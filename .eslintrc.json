{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jasmine": true,
        "wdio-jasmine-framework": true,
        "webdriverio": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "extends": ["airbnb", "plugin:jasmine/recommended", "plugin:webdriverio/recommended", "plugin:wdio-jasmine-framework/recommended"],
    "plugins": [
        "wdio-jasmine-framework",
        "webdriverio",
        "jasmine"
    ]
}
